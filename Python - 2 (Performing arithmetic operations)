#performing arithmetic operators

num1=int(input("Enter first number"))
num2=float(input("Enter second number"))

#addition

add=num1+num2
print("Addition of",num1,"and",num2,"=",add)

#subtraction

sub=num1-num2
print("Subtraction of",num2,"from",num1,"=",sub)

#multiplication

multi=num1*num2
print("Multiplication of",num1,"multiply by",num2,"=",multi)

#division

div=num1/num2
print("Division of",num1,"divide by",num2,"=",div)
